{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist/",
    "strict": true,
    "noImplicitAny": true,
    "module": "es6",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["es2015", "dom", "es6", "es2017"]
  }
}
